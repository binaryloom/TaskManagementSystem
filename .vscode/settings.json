{
  "gitdoc.enabled": true,
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    }
  },
  "isort.args": ["--profile", "black"],
  "files.associations": {
    "*.cfg": "config_files"
  },
  "[config_files]": {
    "editor.formatOnSave": false
  },
  "cSpell.words": [
    "dontlognull",
    "forwardfor",
    "httplog",
    "maxconn",
    "roundrobin",
    "viewsets"
  ]
}
